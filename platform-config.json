        "endpoint": "https://generativelanguage.googleapis.com/v1/models/{model}:generateContent",
        "models": ["gemini-pro", "gemini-pro-1.5"],
        "defaultModel": "gemini-pro",
        "requiresModel": true,
        "authType": "query",
        "authParamName": "key"
      }
    },
    "grok": {
      "name": "Grok",
      "url": "https://grok.com/",
