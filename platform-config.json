      "api": {
        "endpoint": "https://api.grok.ai/v1/chat/completions",
        "models": ["grok-1"],
        "defaultModel": "grok-1",
        "requiresModel": true,
        "authType": "bearer",
        "authHeaderName": "Authorization"
      }
