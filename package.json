{
  "name": "ai-content-assistant",
  "version": "1.0.0",
  "description": "Chrome extension to interact with web content, YouTube, Reddit, & PDFs using multiple AI models via sidebar or popup.",
  "main": "webpack.config.js",
  "scripts": {
    "build": "NODE_ENV=production webpack",
    "build:dev": "webpack",
    "watch": "webpack --watch",
    "lint": "eslint 'src/**/*.{js,jsx}'",
    "lint:fix": "eslint 'src/**/*.{js,jsx}' --fix",
    "format": "prettier --write 'src/**/*.{js,jsx}' '*.{js,json,html,md,css}' --ignore-path .prettierignore",
    "format:check": "prettier --check 'src/**/*.{js,jsx}' '*.{js,json,html,md,css}' --ignore-path .prettierignore"
  },
  "devDependencies": {
    "@babel/core": "^7.26.9",
    "@babel/preset-env": "^7.26.9",
    "@babel/preset-react": "^7.26.3",
    "@tailwindcss/container-queries": "^0.1.0",
    "@tailwindcss/typography": "^0.5.16",
    "autoprefixer": "^10.4.16",
    "babel-loader": "^10.0.0",
    "css-loader": "^6.10.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jscodeshift": "^17.3.0",
    "postcss": "^8.4.31",
    "postcss-loader": "^8.0.0",
    "prettier": "^3.2.5",
    "style-loader": "^3.3.3",
    "tailwindcss": "^3.4.1",
    "webpack": "^5.98.0",
    "webpack-cli": "^6.0.1"
  },
  "dependencies": {
    "gpt-tokenizer": "^2.9.0",
    "katex": "^0.16.22",
    "pdfjs-dist": "^5.2.133",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-katex": "^3.0.1",
    "react-markdown": "^10.1.0",
    "react-syntax-highlighter": "^15.6.1",
    "remark-gfm": "^4.0.1",
    "tiktoken": "^1.0.20",
    "youtube-transcript": "^1.2.1"
  }
}
